<UserControl x:Class="NotesWpfApp.View.UserControls.NavBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NotesWpfApp.View.UserControls"
             mc:Ignorable="d" 
             Height="Auto" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="Auto"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="NoteList" BorderThickness="0" Background="White"  Height="50" Width="60" Content="NoteList" Click="NoteList_Click"/>
        <Button Grid.Column="1" x:Name="AllmyNotes" BorderThickness="0" Background="White" Width="80" Content="AllMyNotes" Click="AllmyNotes_Click"/>
    </Grid>
</UserControl>
